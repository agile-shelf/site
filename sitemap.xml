<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="https://www.sitemaps.org/schemas/sitemap/0.9">
	{% for page in site.pages %}{% if page.url == '/admin' %}{% elsif page.url == '/agenda' %}{% else %}<url>
		<loc>https://www.opde1sterij.nl{{page.url}}</loc>
		<lastmod>{{page.date | date_to_xmlschema}}</lastmod>
		<priority>1.0</priority>
	</url>
	{% for subpage in page.pages %}<url>
		<loc>https://www.opde1sterij.nl{{subpage.url}}</loc>
		<lastmod>{{subpage.date | date_to_xmlschema}}</lastmod>
		<priority>1.0</priority>
	</url>{% endfor %}{% endif %}{% endfor %}
	{% for post in site.posts %}<url>
		<loc>https://www.opde1sterij.nl{{post.url}}</loc>
		<lastmod>{{post.date | date_to_xmlschema}}</lastmod>
		<priority>1.0</priority>
	</url>{% endfor %}
</urlset>
